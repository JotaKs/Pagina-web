<script src="https://code.jquery.com/jquery-1.10.2.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>

  <link rel="stylesheet" href="~/css/main.css" />

  <body class="maincontainer">
  <div id="container">
    <div style="margin-top: 20px">
      <h1 class="text">Welcome to EngageDigital Demo Application</h1>
    </div>
    <div style="text-align: center" id="status"></div>
    <div class="box">
      @*<span class="fieldName">Domain : </span>*@
      <input style="margin-left: 0.8em" class="textfield" type="text" id="engageDomain" size="50"
        placeholder="rtc.radisys.com" value="rtc.engagedigital.ai" hidden />
      <br />
      <span class="fieldName">Nombre de usuario : </span>
      <input class="textfield" type="text" id="userIdentity" size="50" value="bob" />
      <br />
      <p style="font-size: 14px;margin: 0px;padding: 0px;">* No proporcione caracteres especiales ni espacios.</p>
      <br />
      <button id="connectBtn">Connectar</button>
      <button id="disconnectBtn" disabled>Disconectar</button>
      <p />
      <div id="sipIdentity"></div>
    </div>
    <div class="videoContainer">
      <div class="innervideo">
        <div class="watermark">Local Video</div><video id="localVideo" playsinline autoplay muted></video>
      </div>
      <div class="innervideo">
        <div class="watermark">Remote Video</div><video id="remoteVideo" playsinline autoplay></video>
      </div>
    </div>

    <div class="subbox">
      <span class="fieldName">Enter valid service Phone Number :</span>
      <input class="textfield" type="text" id="callTo"
        size="50" placeholder="eg: +919090909090, 18000001234, +14155552671" value="9070707120" hidden/>
      <button id="makeCallBtn" disabled>Call</button>
      <button id="endCallBtn" disabled>Hang Up</button>
    </div>
  </div>
  <div id="incomingCallDialog" class="modal fade">
    <div class="modal-dialog">
      <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h4 id="acceptOrReject" class="modal-title">Incoming Call</h4>
          </div>
          <div class="modal-body">
            <p id="lblMsgConfirmYesNo"></p>
          </div>
          <div class="modal-footer">
            <button id="acceptCallBtn" type="button" class="btn btn-primary">Yes</button>
            <button id="rejectCallBtn" type="button" class="btn btn-default">No</button>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script src="~/js/main.js"></script>

</body>